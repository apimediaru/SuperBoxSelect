{
  "name": "SuperBoxSelect",
  "lowCaseName": "superboxselect",
  "description": "Advanced Listbox/Resource List Template Variable",
  "author": "Benjamin Vauchel, Thomas Jakobi",
  "version": "3.0.8",
  "package": {
    "elements": {
      "plugins": [
        {
          "name": "SuperBoxSelect",
          "description": "SuperBoxSelect runtime hooks - registers custom TV input types and includes javascripts on document edit pages.",
          "file": "superboxselect.plugin.php",
          "events": [
            "OnManagerPageBeforeRender",
            "OnTVInputPropertiesList",
            "OnTVInputRenderList"
          ]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "debug",
        "type": "combo-boolean",
        "area": "system",
        "value": "0"
      },
      {
        "key": "advanced",
        "type": "combo-boolean",
        "area": "system",
        "value": "0"
      }
    ]
  },
  "dependencies": [
    {
      "name": "php",
      "version": ">=7.2"
    },
    {
      "name": "modx",
      "version": ">=2.6"
    }
  ],
  "build": {
    "readme": "docs/readme.md",
    "license": "docs/license.md",
    "changelog": "docs/changelog.md",
    "setupOptions": {
      "source": "setup.options.php"
    },
    "resolver": {
      "after": [
        "resolve.cleanup.php",
        "resolve.events.php",
        "resolve.treehillstudio.php"
      ]
    }
  }
}
